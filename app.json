{
  "expo": {
    "name": "모두의 웨이터 매장",
    "slug": "everyonewaiter-store-app",
    "version": "1.0.8",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "everyonewaiter-store",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f22020"
      },
      "package": "com.everyonewaiter.storeapp"
    },
    "androidStatusBar": {
      "hidden": true,
      "backgroundColor": "#f22020"
    },
    "platforms": [
      "android"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#00000080"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f22020"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "store-app",
          "organization": "everyonewaiter"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d432ad2b-27dd-4214-acc1-556cca6a92ab"
      }
    },
    "owner": "everyonewaiter",
    "ios": {
      "bundleIdentifier": "com.everyonewaiter.storeapp"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d432ad2b-27dd-4214-acc1-556cca6a92ab"
    }
  }
}
